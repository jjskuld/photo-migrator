{
  "name": "photo-migrator",
  "version": "0.1.0",
  "description": "Cross-Platform Apple Photos to Google Photos Uploader",
  "main": "dist/main.js",
  "scripts": {
    "start": "node dist/main.js",
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:fix": "eslint 'src/**/*.{js,ts}' --fix",
    "scan": "node dist/main.js scan",
    "upload": "node dist/main.js upload",
    "login": "node dist/main.js login",
    "status": "node dist/main.js status",
    "upload:photos": "node dist/main.js upload --photos-only",
    "upload:videos": "node dist/main.js upload --videos-only",
    "build:swift": "cd swift/MediaScannerMac && swift build -c release && mkdir -p ../../bin && cp ../.build/arm64-apple-macosx/release/MediaScannerMac ../../bin/",
    "test:swift": "cd swift/MediaScannerMac && swift test"
  },
  "keywords": [
    "photos",
    "google-photos",
    "apple-photos",
    "uploader",
    "migration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@types/async-retry": "^1.4.9",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.14.1",
    "@types/yargs": "^17.0.33",
    "async-retry": "^1.3.3",
    "axios": "^1.8.4",
    "better-sqlite3": "^11.9.1",
    "google-auth-library": "^9.15.1",
    "keytar": "^7.9.0",
    "typescript": "^5.8.3",
    "winston": "^3.17.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "c8": "^10.1.3",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "^3.5.3",
    "typescript-eslint": "^8.30.1",
    "vitest": "^3.1.1"
  }
}
